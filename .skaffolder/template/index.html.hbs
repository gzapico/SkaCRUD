{
    "forEachObj": "oneTime",
    "overwrite": false,
    "_partials": [
        {
            "template": "{{#each resources}}{{#each _resources}}\n\t\t<script src=\"./js/services/{{name}}Service.js\"></script>\n\t\t<script src=\"./js/services/custom/{{name}}CustomService.js\"></script>\n{{/each}}{{/each}}\n    ",
            "tagTo": "<!-- END IMPORT SERVICES -->",
            "tagFrom": "<!-- START IMPORT SERVICES -->",
            "name": "Partial Service"
        },
        {
            "tagTo": "<!-- END IMPORT CONTROLLERS -->",
            "tagFrom": "<!-- START IMPORT CONTROLLERS -->",
            "template": "{{#each modules}}\n\t\t<script src=\"./js/controllers/{{name}}Controller.js\"></script>\n{{/each}}\n    ",
            "name": "Partial Controller"
        },
        {
            "template": "\n\t{{#each modules}}\n\t{{#startsWith \"List_\" template}}\n                        <li><a ng-href=\"#{{url}}\">{{capitalize _resources.0.name}}</a></li>\n                        {{/startsWith}}\n                        {{/each}}\n                    \n                    ",
            "tagTo": "<!-- END LINK MENU -->",
            "tagFrom": "<!-- START LINK MENU -->",
            "name": "Link Menu"
        }
    ]
}
**** END PROPERTIES SKAFFOLDER ****
